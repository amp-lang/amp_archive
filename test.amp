    var foo1 = Foo .{ member = 0 };
    var foo2 = Foo .{ member = 10 };

    while foo1 != foo2 {
        foo1.member = foo1.member + 1;
        // printf("%hu\n", foo1.member);
    }

    printf("TEST %hu\n", foo1.member);
}

