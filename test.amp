struct MyStruct {
    member: i32,
}

struct Box {
    member: MyStruct,
}

func Box() -> Box {
    return Box .{ member = MyStruct .{ member = 0 } };
}

func SetI32(dst: ~mut i32, value: i32) {
    *dst = value;
}

func printf(str: ~const u8, arg: i32);

func Main() {
    var box = Box();
    SetI32(~mut box.member.member, 64);
    printf("%d\n", box.member.member);
}